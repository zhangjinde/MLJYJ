<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.itcast.domain.app">

	<class name="FamilyVideo" dynamic-insert="true" >
		<id name="fv_id">
			<generator class="native"/>
		</id>
		<property name="fv_title"/>
		<property name="fv_introduction"/>
		<property name="fv_videourl"/>
		<property name="fv_place"/>
		<property name="fv_time"/>
		<property name="fv_familyid"/>
		<property name="state"/>
		
		
	</class>

</hibernate-mapping>