<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.itcast.domain.app">

	<class name="AppActivity" dynamic-insert="true" >
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="imgurl"/>
		<property name="jumpurl"/>
		<property name="studioid"/>
		<property name="isuse" type="java.lang.Integer">
			<column name="isuse" not-null="true" default="0"/> 
		</property>
	
	</class>

</hibernate-mapping>