<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.itcast.domain">

	<class name="Pictures">
		<id name="pic_id">
			<generator class="native" />

		</id>
		<property name="pic_uuid"/>
		<property name="pic_realname"/>
		<property name="pic_fullurl"/>
		<property name="pic_url"/>
		<property name="pic_relurl"/>
		<property name="upload_time"/>
		<property name="pic_instructions"/>
		<property name="upload_userid"/>
		<property name="pic_index"/>
		<many-to-one name="pic_pa" column="pic_pa"/>
		
		
		
		
	</class>

</hibernate-mapping>